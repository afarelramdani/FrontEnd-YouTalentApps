                android:textColor="@android:color/white"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/tv_skill1_worker2_home"
                app:layout_constraintRight_toLeftOf="@id/tv_skill4_worker2_home"
                app:layout_constraintLeft_toRightOf="@id/iv_worker2"/>
            <TextView
                android:id="@+id/tv_skill4_worker2_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
