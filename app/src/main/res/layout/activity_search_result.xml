<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".search.SearchResultActivity">

        <Button
            android:id="@+id/btn_back_search"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/ic_baseline_arrow_back_ios_24"
            />

        <SearchView

            android:id="@+id/sv_search_talent_result"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:iconifiedByDefault="false"
            android:queryHint="Search Here"
            app:layout_constraintLeft_toRightOf="@id/btn_back_search"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp" />

        <TextView
            android:id="@+id/tv_search_talent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android Developer"
            android:textStyle="bold"
            android:textSize="30sp"
            android:layout_marginTop="50dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/sv_search_talent_result" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame_1_home"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_margin="30dp"
            android:layout_marginTop="56dp"
            android:background="#FFFFFF"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_search_talent">

            <ImageView
                android:id="@+id/iv_worker"
                android:layout_width="100dp"
                android:layout_height="100dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintLeft_toRightOf="@id/tv_skill1_worker_home"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="25dp"
                android:layout_marginRight="200dp"
                android:src="@drawable/dalmiku"/>

            <TextView
                android:id="@+id/tv_type_worker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                android:textStyle="bold"
                android:gravity="left"
                android:text="Web Developer"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="90dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/iv_worker"/>
            <TextView
                android:id="@+id/tv_name_worker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="right"
                android:text="Ahmad Farhan El Ramdhani"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="40dp"
                android:layout_marginRight="90dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/iv_worker"/>


            <TextView
                android:id="@+id/tv_skill1_worker_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/orange_skill"
                android:padding="4dp"
                android:gravity="center"
                android:text="NodeJS"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                app:layout_constraintLeft_toRightOf="@+id/iv_worker"
                app:layout_constraintRight_toLeftOf="@+id/tv_skill2_worker_home"
                app:layout_constraintTop_toBottomOf="@+id/tv_name_worker" />

            <TextView
                android:id="@+id/tv_skill2_worker_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:background="@color/orange_skill"
                android:padding="4dp"
                android:gravity="center"
                android:text="NodeJS"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/tv_name_worker"
                app:layout_constraintLeft_toRightOf="@+id/tv_skill1_worker_home"
                app:layout_constraintRight_toRightOf="parent"
                tools:layout_editor_absoluteY="70dp" />

            <TextView
                android:id="@+id/tv_skill3_worker_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/orange_skill"
                android:padding="4dp"
                android:gravity="center"
                android:text="NodeJS"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/tv_skill1_worker_home"
                app:layout_constraintRight_toLeftOf="@id/tv_skill4_worker_home"
                app:layout_constraintLeft_toRightOf="@id/iv_worker"/>
            <TextView
                android:id="@+id/tv_skill4_worker_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:background="@color/orange_skill"
                android:padding="4dp"
                android:gravity="center"
                android:text="NodeJS"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@+id/tv_skill2_worker_home"
                app:layout_constraintLeft_toRightOf="@+id/tv_skill3_worker_home"
                app:layout_constraintRight_toRightOf="parent"
                tools:layout_editor_absoluteY="70dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

