            <TextView
                android:id="@+id/tv_skill3_worker2_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/orange_skill"
                android:padding="4dp"
                android:gravity="center"
